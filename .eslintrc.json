{
  "env": {
    "node": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": ["eslint:recommended", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier", "jest"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": ["error"]
  }
}
